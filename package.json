{
  "name": "my-fullstack-app",
  "version": "1.0.0",
  "description": "portfolio website",
  "scripts": {
    "start:backend": "cd server && npm start server",
    "dev:backend": "cd backend && npm start server",
    "prod:backend": "NODE_ENV=production nodemon server.js",
    "start:frontend": "cd client && npm start",
    "build:frontend": "cd client && npm run build",
    "deploy": "npm run build:frontend && npm run start:backend",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "axios": "^1.4.0",
    "http-proxy-middleware": "^2.0.6",
    "jodit-react": "^1.3.39",
    "nodemon": "^3.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-loader-spinner": "^5.3.4",
    "react-quill": "^2.0.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "react-simple-typewriter": "^5.0.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "typed.js": "^2.0.16",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
